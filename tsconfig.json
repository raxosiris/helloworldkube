{
    "compilerOptions": {
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "module": "commonjs",
      "outDir": "dist",
      "baseUrl": ".",
      "esModuleInterop": true,
      "target": "es6",
      "noImplicitAny": false,
      "moduleResolution": "node",
      "sourceMap": true,
      "lib": ["esnext"],
      "strict": false,
      "noUnusedLocals": true,
      "skipLibCheck": true,
      "skipDefaultLibCheck": true
    }
  }